#include <stdio.h>
int DMG(int D,int M,int G)
{
    int sum=365*(G);
    
    if(M==1)
    {
        D=D;
    }
    if (M==2)
    {
        D+=31;
    }
    if (M==3)
    {
       D+=31+28;
    }
    if (M==4)
    {
        D+=31*2+28;
    }
    if (M==5)
    {
        D+=31*2+28+30;
    }
    if (M==6)
    {
        D+=31*3+28+30;
    }
    if (M==7)
    {
        D+=31*3+28+30*2;
    }
    if (M==8)
    {
        D+=31*4+28+30*2;
    }
    if (M==9)
    {
        D+=31*5+28+30*2;
    }
    if (M==10)
    {
        D+=31*5+28+30*3;
    }
    if (M==11)
    {
        D+=31*6+28+30*3;
    }
    if (M==12)
    {
        D+=31*6+28+30*4;
    }
    sum+=D;
    return sum;
}
int main() 
{
    int D,M,G;
    int Dm,Mm,Gm;
    printf("Введите свою дату рождения:\n");
    scanf("%d %d %d",&Dm,&Mm,&Gm);
    printf("Введите текущую дату:\n");
    scanf("%d %d %d",&D,&M,&G);
    int C=DMG(D,M,G)-DMG(Dm,Mm,Gm);
    printf("Прошло %d суток",C);
    
}
